@model Bagaround.Models.HomeListItem

@using (Html.BeginForm("SetSessionSearch", "SelectedAndSearch", FormMethod.Post))
{
    <div class="input-group">
        @Html.TextBoxFor(x => x.Search.SearchKey, new { @Class = "form-control", placeholder = "Looking For" })
        <span class="input-group-addon"><button type="submit" style="border:none; background:none"><i class="fa fa-search" aria-hidden="true"></i></button></span>
    </div>
}